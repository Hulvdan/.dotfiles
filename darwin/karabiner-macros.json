{
  "description": "New Rule (change left_shift+caps_lock to page_down, right_shift+caps_lock to left_command+mission_control)",
  "manipulators": [
    {
      "type": "basic",
      "from": { "key_code": "f20" },
      "to": [
        {
          "set_variable": {
            "name": "caps_pressed",
            "value": 1,
            "key_up_value": 0
          }
        }
      ]
    }

